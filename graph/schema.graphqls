# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Target {
 target : String!
 lastModified: String!
 status: String!
}

type Query {
  targets: [Target!]!
  subDomain(target: String!): [Target!]!
  runScan(target: String!): [Target!]!
}

input TargetInput {
  target: String!
}

type Mutation {
  addTarget(input: TargetInput!): Target!
  removeTarget(input: TargetInput!): Target!
}

